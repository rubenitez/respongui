{
  "QCustomSlideMenu": [
    {
      "name": "menu_widget",
      "defaultSize": [
        {
          "width": "250",
          "height": "auto"
        }
      ],
      "collapsedSize": [
        {
          "width": 0,
          "height": 0
        }
      ],
      "expandedSize": [
        {
          "width": "250",
          "height": "auto"
        }
      ],
      "toggleButton": [
        {
          "buttonName": "open_close_side_bar_btn",
          "icons": [
            {
              "whenMenuIsCollapsed": ":/icons/icons/align-left.svg",
              "whenMenuIsExpanded": ":/icons/icons/chevron-left.svg"
            }
          ],
          "style": [
            {
              "whenMenuIsCollapsed": [
                "border: 2px solid transparent"
              ],
              "whenMenuIsExpanded": [
                "border: 2px solid rgb(9, 5, 13)"
              ]
            }
          ]
        }
      ],
      "menuTransitionAnimation": [
        {
          "animationDuration": 1000,
          "animationEasingCurve": "OutInBack"
        }
      ],
      "menuContainerStyle": [{
        "whenMenuIsExpanded": [
          "background-color: rgb(9, 5, 13);"
        ]
      }]
    },
    {
      "name": "right_menu",
      "default_Size": [
        {
          "width": 200,
          "height": "auto"
        }
      ],
      "collapsedSize": [
        {
          "width": 0,
          "height": "auto"
        }
      ],
      "expandedSize": [
        {
          "width": 200,
          "height": "auto"
        }
      ],
      "toggleButton": [
        {
          "buttonName": "right_menu_toggle_btn",
          "icons": [
            {
              "whenMenuIsCollapsed": ":/icons/icons/settings.svg",
              "whenMenuIsExpanded": ":/icons/icons/chevron-right.svg"
            }
          ]
        }
      ],
      "menuTransitionAnimation": [
        {
          "animationDuration": 500,
          "animationEasingCurve": "Linear"
        }
      ]
    },
    {
      "name": "search_widget",
      "defaultSize": [
